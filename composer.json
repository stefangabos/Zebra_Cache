{
    "name": "stefangabos/zebra_cache",
    "type": "library",
    "description": "A file-based lightweight PHP caching library that uses file locking to ensure proper functionality under heavy load.",
    "keywords": ["php", "cache", "caching", "filesystem"],
    "homepage": "https://github.com/stefangabos/Zebra_Cache",
    "license": "LGPL-3.0-only",
    "authors": [
        {
            "name": "Stefan Gabos",
            "email": "contact@stefangabos.ro",
            "homepage": "https://github.com/stefangabos",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0"
    },
    "autoload": {
        "psr-4": {
            "stefangabos\\Zebra_Cache\\": "./"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.8",
        "squizlabs/php_codesniffer": "*"
    },
    "support": {
        "issues": "https://github.com/stefangabos/Zebra_Cache/issues",
        "source": "https://github.com/stefangabos/Zebra_Cache"
    },
    "config": {
        "sort-packages": true
    }
}
